{
	"name": "addwiki/mediawiki-api",
	"type": "library",
	"description": "A Mediawiki api lib",
	"keywords": ["Mediawiki"],
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Addshore"
		}
	],
	"require": {
		"addwiki/mediawiki-api-base": "~2.0",
		"addwiki/mediawiki-datamodel": "~0.6.0"
